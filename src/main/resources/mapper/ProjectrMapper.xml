<mapper namespace="com.example.h_item.mapper.ProjectMapper">

    <insert id="insert"
            parameterType="com.example.h_item.model.po.ProjectPO"
            keyProperty="id" useGeneratedKeys="true">
        insert into `user`(`pro_id`, `pro_name`, `sex`, `birthday`, `img`,
                           `academy_id`, `educational`, `major`, `phone`, `remark`)
        values (#{username}, #{password}, #{sex}, #{birthday}, #{img}, #{academyId},
                #{educational}, #{major}, #{phone}, #{remark})
    </insert>

    <select id="queryById"
            resultType="com.example.h_item.model.po.UserPO">
        select `id`,
               `username`,
               `password`,
               `sex`,
               `birthday`,
               `img`,
               `academy_id`,
               `educational`,
               `major`,
               `phone`,
               `remark`
        from `user`
        where `id` = #{id}
    </select>

    <select id="queryByUsername"
            resultType="com.example.h_item.model.po.UserPO">
        select `id`,
               `username`,
               `password`,
               `sex`,
               `birthday`,
               `img`,
               `academy_id`,
               `educational`,
               `major`,
               `phone`,
               `remark`
        from `user`
        where `username` = #{username}
    </select>


